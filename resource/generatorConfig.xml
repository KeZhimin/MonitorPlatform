<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<!-- 引入配置文件 -->
	<properties resource="db.properties" />

	<!-- 指定数据库连接驱动 -->
	<classPathEntry location="E:\Elipse Workspace\MonitorPlatform\WebContent\WEB-INF\lib\ojdbc14.jar" />




	<context id="context1">

		<commentGenerator>
			<property name="suppressAllComments" value="true" /><!-- 是否取消注释 -->
			<property name="suppressDate" value="true" /> <!-- 是否生成注释代时间戳 -->
		</commentGenerator>


		<!-- 连接数据库配置，可以使用${}这个方法来获取properties中的值 -->
		<jdbcConnection driverClass="oracle.jdbc.OracleDriver"
			connectionURL="jdbc:oracle:thin:@172.168.68.251:1521:btss" userId="btss_monitor"
			password="btss_monitor" />

		<!-- 生成实体类 -->
		<javaModelGenerator targetPackage="com.ffcs.mp.entity"
			targetProject="MonitorPlatform" />

		<!-- 生成Dao接口 -->
		<sqlMapGenerator targetPackage="com.ffcs.mp.dao"
			targetProject="MonitorPlatform" />

		<!-- 生成mapper.xml -->
		<javaClientGenerator targetPackage="mapper"
			targetProject="MonitorPlatform" type="XMLMAPPER" />

		<!-- 配置表生成对应代码 -->
		<table tableName="APP_SEVER_MONITOR_LOG" domainObjectName="AppServerMonitorLog"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<property name="ignoreQualifiersAtRuntime" value="false" />
			<property name="useActualColumnNames" value="true" />
		</table>

	</context>
</generatorConfiguration>