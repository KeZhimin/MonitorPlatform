<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 数据库方言 -->
	<properties>
		<property name="dialect" value="oracle" />
	</properties>


	<!-- 分页插件 -->
	<plugins>
		<plugin
			interceptor="com.ffcs.icity.common.mybatis.interceptor.PaginationInterceptor" />
		<plugin
			interceptor="com.ffcs.icity.common.mybatis.interceptor.PaginationResultSetInterceptor" />
	</plugins>

</configuration>